{
    "name": "segfaultinc/finite",
    "description": "Simple yet powerful Finite State Machine implementation",
    "keywords": [
        "segfaultinc",
        "finite"
    ],
    "homepage": "https://github.com/segfaultinc/finite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jerguslejko",
            "email": "jergus.lejko@gmail.com",
            "homepage": "https://jerguslejko.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SegfaultInc\\Finite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SegfaultInc\\Finite\\Tests\\": "tests"
        }
    },
    "scripts": {
